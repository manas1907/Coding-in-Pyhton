class calculation1:
    def summation(self,a,b):
        return a+b;
class calculation2:
   def multiplication (self,a,b):
       return a*b;
class Derived(calculation1,calculation2):
    def divide(self,a,b):
        return a/b;
d = Derived()
print(issubclass(Derived, calculation2))
print(isinstance(d,calculation1))