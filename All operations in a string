thislist = [100, 50, 65, 82, 23]
thislist.sort()
print(thislist)
thislist.append(30)
print(thislist)
thislist.insert(1, 30)
print(thislist)
thislist.remove(100)
print(thislist)
thatlist = [20, 30, 55, 40, 66]
thislist.extend(thatlist)
print(thislist)
thislist.pop()
print(thislist)
thislist.count(30)
print(thislist)
thislist.sort()
print(thislist)
x =max(thislist)
print(x)
a = min(thislist)
print(a)
